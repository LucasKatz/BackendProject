<div>
        <h1 class="text-white text-center">Lista de productos</h1>
        <div>
            {{#each products.payload}}
                    <div>
                      <h2 class="card-title">{{this.title}}</h2>
                        <div class="card-body">
                            <img src="{{this.thumbnails}}">
                        </div>
                    </div>
                </div>
            {{/each}}
        </div>

        {{#if products.pagination.active}}
            <nav>
                <ul class="pagination">
                    <li class="page-item">
                        <a class="page-link" href="{{products.pagination.prevLink}}" aria-label="Previous">
                            <span aria-hidden="true">&laquo;</span>
                        </a>
                    </li>
                    {{#each products.pagination.pagesLinks}}
                        <li class="page-item"><a class="page-link{{#if active}} active {{/if}}" href="{{link}}">{{page}}</a></li>
                    {{/each}}
                    <li class="page-item">
                        <a class="page-link" href="{{products.pagination.nextLink}}" aria-label="Next">
                            <span aria-hidden="true">&raquo;</span>
                        </a>
                    </li>
                </ul>
            </nav>
        {{/if}}
    </div>
    <button id="logout">Logout</button>
</div>

<script src="logout.js" type="application/javascript"></script>